              document.getElementById('GETemplate:GETemplate:mainForm:expensePageBlock1:Expense__c-1:component-1:Expense__c-mycomponent:rentingOnly').style.display = "none";
              document.getElementById('GETemplate:GETemplate:mainForm:expensePageBlock1:Expense__c-1:component-1:Expense__c-mycomponent:rentingBlockBoard').style.display = "block";
