              document.getElementById('GETemplate:GETemplate:mainForm:expensePageBlock1:Expense__c-1:component-1:Expense__c-mycomponent:rentingBlockBoard').style.display = "none";

           }else if((resStatus.value == 'Boarding with Parents' || resStatus.value == 'Boarding Other') && ('{!country_NZ}' != '{!region}')){
