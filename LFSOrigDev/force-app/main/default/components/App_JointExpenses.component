       function totalExpenseZero(){
        var expTotalAU = document.getElementById('totalMonthlyLivingExpense');
        var expTotal = document.getElementById('totalMonthlyLivingExpenseNZ');
        var anyaddExpNZ = document.getElementById('GETemplate:GETemplate:mainForm:expensePageBlock1:Expense__c-1:component-1:Expense__c-mycomponent:anyAddExpNZ');
        if('{!country_NZ}' == '{!region}'){
            if(expTotal.value < 1 && anyaddExpNZ.value != ''){
             document.getElementById('showErrExpense').style.display='block';
             document.getElementById('showErrExpense').innerHTML ="<h5 class='totalExpenseValidation' style='font-family:Helvetica,Arial,sans-serif;'>Total residential and living expenses must be greater than $0</h5>";
             return false;
            }else{
             document.getElementById('showErrExpense').style.display='none';
            } 
            
        }
        if('{!country_NZ}' != '{!region}'){       
            var persExpVal = document.getElementById('GETemplate:GETemplate:mainForm:expensePageBlock1:Expense__c-1:component-1:Expense__c-mycomponent:persExpVal');
            if(expTotalAU.value < 1 && persExpVal.value != ''){
             document.getElementById('showErrExpense').style.display ='block';
             document.getElementById('showErrExpense').innerHTML = "<h5 style='font-family:Lexia Bold;'>Total living expenses must be greater than $0</h5>";
             return false;
            }else{
             document.getElementById('showErrExpense').style.display ='none';
            }
            
        }
        if(loanAmtBSAU() == false && "{!$CurrentPage.parameters.subType}" == "betterStart"){
         return false;
        }else{       
         return true;
        }
       }
