function setHigherRepaymentAmt(field){    
    var repaymentYes = document.getElementById("repaymentYes");
    var repaymentNo = document.getElementById("repaymentNo");
    
    if (field.innerHTML.trim() == 'YES') {
        repaymentYes.className = 'btn btn-block GForm-group GForm-field body-label GForm-half alignleft YesSelected';
        repaymentNo.className = 'btn btn-block GForm-group GForm-field body-label GForm-half alignright NoOp';
    } else if (field.innerHTML.trim() == 'NO') {
        repaymentYes.className = 'btn btn-block GForm-group GForm-field body-label GForm-half alignleft YesOp';
        repaymentNo.className = 'btn btn-block GForm-group GForm-field body-label GForm-half alignright NoSelected';
    }
    
}
    
