$(document).ready(function () {
        
    var mode;
    var checkMode = function () {
        
        if ($(window).width() <= 414 ) {         
            
        $(".icon_profile").removeClass('icon_prof_lg_size');
        $(".icon_profile").addClass('icon_prof_sm_size');
            
        $(".icon_logo").removeClass('logo_lg_size');
        $(".icon_logo").addClass('logo_sm_size');
            
         $("#btnFile,#btnFile2,#btnFile3").removeClass('margin_file').addClass('pull-left');
         $("#btnSub,#btnSub2,#btnSub3").addClass('pull-right');    
            
         $("#progresslbl").removeClass('aligntext');
         $("#toplogo").removeClass('aligntext');
         $("#logout").removeClass('aligntext');
    
        $("#pbar").removeClass('progress_style');
        $("#pbar").addClass('progress_style_mobile');
        
        $("#thumbsUp").removeClass('icon_middle_width_lg');
        $("#thumbsUp").addClass('icon_approved_thumbsup_width_sm');  
            
        $("#brandArrow").removeClass('icon_brand_down_arrow_width_lg');
        $("#brandArrow").addClass('icon_brand_down_arrow_width_sm'); 
      
        $("#intro").removeClass('gap-text');
            
        $("#contLabela, #contLabelc").addClass('pull-right');
     
        $("#aBack").removeClass('underline-dotted');

        $("#profile").hide();
            
       $("#btnDiv, #btnDone, #btnBack").removeClass('pull-right');
       $("#btnDiv, #btnDone, #btnBack").addClass('text-center');
            
       $("#btnConfDiv,#btnConfDiv2,#btnConfDiv3").removeClass('vertical-align');   
       $("#btnConfDiv,#btnConfDiv2,#btnConfDiv3").removeClass('pull-right');   
       $("#btnConfDiv,#btnConfDiv2,#btnConfDiv3").removeClass('width_file_container');   
            
        $(".hr-solid-border").show();
        $("#hdrLoan").hide();   
    
            
        } else{
            
            
        $(".icon_profile").addClass('icon_prof_lg_size');
        $(".icon_profile").removeClass('icon_prof_sm_size');
            
        $(".icon_logo").addClass('logo_lg_size');
        $(".icon_logo").removeClass('logo_sm_size');
     
        $("#profile").show();
        $("#progresslbl").addClass('aligntext');
        $("#toplogo").addClass('aligntext');
        $("#logout").addClass('aligntext');
        $("#toprow").addClass('hdr');
            
        $("#pbar").addClass('progress_style');
        $("#pbar").removeClass('progress_style_mobile');

        $("#loanRow").addClass('vertical-align'); 

        $("#thumbsUp").addClass('icon_middle_width_lg');
        $("#thumbsUp").removeClass('icon_approved_thumbsup_width_sm');  

        $("#brandArrow").addClass('icon_brand_down_arrow_width_lg');
        $("#brandArrow").removeClass('icon_brand_down_arrow_width_sm'); 

        $("#contLabela, #contLabelc").removeClass('pull-right');

        $("#aBack").addClass('underline-dotted');

        $("#l1, #l2, #l3, #l4").show(); 

        $("#btnDiv, #btnDone, #btnBack").addClass('pull-right');
        $("#btnDiv, #btnDone, #btnBack").removeClass('text-center');

        $("#btnFile,#btnFile2,#btnFile3").addClass('margin_file').removeClass('pull-left');
        $("#btnSub,#btnSub2,#btnSub3").removeClass('pull-right');      
            
        $("#btnConfDiv,#btnConfDiv2,#btnConfDiv3").addClass('vertical-align');   
        $("#btnConfDiv,#btnConfDiv2,#btnConfDiv3").addClass('pull-right');   
        $("#btnConfDiv,#btnConfDiv2,#btnConfDiv3").addClass('width_file_container');  
            
        $("#icon_profile").addClass('logo_size');  
        $("#icon_logo").addClass('logo_size');  

        $("#hdrLoan").show(); 
            
        }
       
    };

    var compareMode = function () {
        if (mode !== checkMode()) {
            mode = checkMode();

            $('.mode-informer').text(mode).animate({
                bottom: '100'
            }, 100, function () {
                $('.mode-informer').animate({bottom: 10}, 100)
            });
        }
    };

    $(window).on('resize', function () {
        compareMode()
    });

    compareMode();

});


