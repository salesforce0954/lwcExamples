<!--
    * @Description: Personal Details Component in APP Wizard Page
    * @Author: Davey Yu
    * @Date Created: 07-OCT-2015
    * @History:
    =====================================================================
        07-OCT-15: Created - Davey
        22-FEB-16: Updated - Davey - Defect 280 - Allow hyphen to street fields for AU Application 
                   from 'isName' to 'isStreetName'
    ===================================================================== 
-->
<apex:component >   
    <style type="text/css">
        .applicantDetails1{
            border-style: solid;
            border-width: 1px; 
            width: 90%;
        }
        .applicantDetails2{
            border-style: solid;
            border-width: 1px; 
            width: 90%;
            margin-left: 3%;
            opacity: 0.6;
        }
        .errClass{
            color:red;
        }
        .applicantDetailHeaderColor{
            background-color: #a8d0da;
        }
    </style>
        
    <!-- START OF APPLICATION DETAILS -->
    <div id="ApplicantDetailsJointSection" class="slds-grid" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
        
        <!-- START OF APPLICANT 1 -->
        <section id='app1DetailsSection' class='slds-col slds-size--1-of-2 applicantDetails1 sectionBorder'>
             <div class="slds-m-bottom--medium slds-box slds-text-align--center applicantDetailHeaderColor">
                <div id='NavigationImageDiv'>
                    <img src="{!$Resource.navCircle}"/>
                </div>
                <div id='NavigationNumberText'>
                   2
                </div>
                <div class="slds-text-heading slds-m-vertical--small sectionHeadingJoint">Personal details</div>
            </div>
            <!-- START OF APPLICANT DETAILS -->
            <div class="slds-grid slds-wrap slds-m-vertical--xx-small">
                <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                   <label class="slds-form-element__label req" for="app1_title__c">Title</label>
                </section>
                <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                 <div class="slds-form-element__control" style="width:30%;">
                     <select class="slds-select isPicklistSpace" id="app1_title__c" name="app1_title__c"/>
                 </div>
                </section>
            </div>
            <div class="slds-grid slds-wrap slds-m-vertical--xx-small">
                <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                   <label class="slds-form-element__label req" for="app1_first_name__c">First name</label>
                </section>
                <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                    <div class="slds-form-element__control" style="width:60%;">
                      <input id="app1_first_name__c" name="app1_first_name__c" class="slds-input isRequired isMinLength2 isMaxLength15 isPersonalName" type="text"/>
                    </div>
                </section>
            </div>
            <div class="slds-grid slds-wrap slds-m-vertical--xx-small">
                <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                   <label class="slds-form-element__label" for="app1_middle_name__c">Middle name</label>
                </section>
                <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                    <div class="slds-form-element__control" style="width:60%;">
                        <input id="app1_middle_name__c" name="app1_middle_name__c" class="slds-input isPersonalName isMaxLength15" type="text"/>
                    </div>
                </section>
            </div>
            <div class="slds-grid slds-wrap slds-m-vertical--xx-small">
                <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                   <label class="slds-form-element__label req" for="app1_last_name__c">Last name</label>
                </section>
                <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                    <div class="slds-form-element__control" style="width:60%;">
                      <input id="app1_last_name__c" name="app1_last_name__c" class="slds-input isRequired isPersonalName isMinLength2 isMaxLength20" type="text" onkeyup="copyTooltip(this, 'app2_last_name_tooltip__c');"/>
                    </div>
                </section>
            </div>
             <div class="slds-grid slds-wrap slds-m-vertical--xx-small">
                <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                    <label class="slds-form-element__label"></label>    
                </section>
                <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                    <div class="slds-form--inline">
                        <a href="#" class="slds-button slds-button--neutral" style="width:30%" id="app1_male_gender" onclick="toggleGender(this, '1'); return false;">Male</a>
                        <a href="#" class="slds-button slds-button--neutral" style="width:30%" id="app1_female_gender" onclick="toggleGender(this, '1'); return false;" onblur="checkGender(this, '1'); return false;">Female</a>
                        <label class="slds-hide errClass" style="font-color:red;" id="app1_gender_error">Please choose your gender.</label>
                        <select class="slds-select slds-hide" id="app1_gender__c" name="app1_gender__c"/>
                    </div>
                </section>
            </div>
            <div class="slds-grid slds-wrap slds-m-vertical--xx-small">
                <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                   <label class="slds-form-element__label req">Date of birth</label>
                </section>
                <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                   <div class="slds-form-element__control" style="width:100%">
                         <select class="slds-select dobday isPicklist" id="app1_dob_dd" name="app1_dob_dd" style="width:20%"/>
                         <select class="slds-select dobmonth isPicklist" id="app1_dob_months" name="app1_dob_months" style="width:20%"/>
                         <input id="app1_dy" name="app1_dy" class="slds-input dobyear isRequired isNumber isYear isApp1DOB18 isApp1DOB110" type="text" style="width:19%" onkeypress="return isNumberKey(event)" maxlength="4"/>
                    </div>
                </section>
            </div>
            <!-- START OF PRIMARY IDENTIFICATION -->
            <div class="{!if($CurrentPage.parameters.country='AU', 'slds-hide', 'slds-show')}" id="app1_div_identification">
                <div class="slds-grid slds-wrap slds-m-vertical--xx-small">
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--left">
                          <label class="slds-form-element__label"></label>
                    </section>
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                         <h3 class="slds-text-heading--small slds-m-vertical--small">Pre Submit Identification</h3>
                    </section>
                </div>
                <div class="slds-grid slds-wrap slds-m-vertical--xx-small">
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                        <label class="slds-form-element__label" for="app1_customer_id_type__c">Customer ID type</label>
                    </section>
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                     <div class="slds-form-element__control" style="width:30%">
                         <select class="slds-select" id="app1_customer_id_type__c" name="app1_customer_id_type__c" onchange="showSecondaryIdentification(this, '1');assignClassToIdentificationFields(this, '1');"/>
                     </div>
                    </section>
                </div>
                <div class="slds-hide" id="app1_div_customer_id_details">
                    <div class="slds-grid slds-wrap slds-m-vertical--xx-small" id="app1_div_id_number">
                        <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                              <label class="slds-form-element__label req" for="app1_id_number__c" id="app1_id_number_label">ID number</label>
                        </section>
                        <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                            <div class="slds-form-element__control" style="width:60%">
                               <input id="app1_id_number__c" name="app1_id_number__c" class="slds-input isAlphaNumericOnly" 
                                onkeypress="return isIDNumber(this, event)" type="text"/>
                            </div>
                        </section>
                    </div>
                    <div class="slds-grid slds-wrap slds-m-vertical--xx-small" id="app1_div_version_number">
                        <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                              <label class="slds-form-element__label req" for="app1_version__c" id="app1_version__c_label">Version number</label>
                        </section>
                        <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                            <div class="slds-form-element__control" style="width:60%">
                               <input id="app1_version__c" name="app1_version__c" class="slds-input isRequired isNumberWithoutDecimal isMinLength3" maxlength="3" type="text"/>
                            </div>
                        </section>
                    </div>
                    <div class="slds-grid slds-wrap slds-m-vertical--xx-small" id="app1_div_residency_number"> 
                        <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                              <label class="slds-form-element__label" for="app1_nz_residency_number__c" id="app1_nz_residency_number__c_label">NZ residency number</label>
                        </section>
                        <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                            <div class="slds-form-element__control" style="width:60%">
                               <input id="app1_nz_residency_number__c" name="app1_nz_residency_number__c" class="slds-input isRequired isAlphaNumeric isMaxLength10" 
                                    onkeypress="return isAlphaNumericNZ(event)" maxlength="10" type="text"/>
                            </div>
                        </section>
                    </div>
                    <div class="slds-grid slds-wrap slds-m-vertical--xx-small" id="app1_div_issue_date"> 
                        <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                           <label class="slds-form-element__label">Issue date</label>
                        </section>
                        <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                            <div class="slds-form-element__control" style="width:100%;">
                                <select class="slds-select" id="app1_issue_dd" name="app1_issue_dd" style="width:20%;"/>  
                                <select class="slds-select" id="app1_issue_months" name="app1_issue_months" style="width:20%;"/>  
                                <input id="app1_issue_dy" name="app1_issue_dy" class="slds-input isIssueDate" type="text" style="width:19%;" onkeypress="return isNumberKey(event)" maxlength="4"/>
                            </div>
                        </section>
                    </div>
                    <div class="slds-grid slds-wrap slds-m-vertical--xx-small" id="app1_div_expiry_date">
                        <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                           <label class="slds-form-element__label" id="app1_expiry_date_label">Expiry date</label>
                        </section>
                        <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                            <div class="slds-form-element__control" style="width:100%;">
                                 <select class="slds-select app1expdd" id="app1_expiry_dd" name="app1_expiry_dd" style="width:20%;"/>
                                 <select class="slds-select app1expmm" id="app1_expiry_months" name="app1_expiry_months" style="width:20%;"/>
                                 <input id="app1_exp_dy" name="app1_exp_dy" class="slds-input app1expyy isExpiryDate" type="text" style="width:19%;"
                                  maxlength="4" onkeypress="return isNumberKey(event)"/>
                            </div>
                        </section>
                    </div>
                    <div class="slds-grid slds-wrap slds-m-vertical--xx-small" id="app1_div_issuing_country">
                        <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                              <label class="slds-form-element__label" for="app1_issuing_Country__c" id="app1_issuing_Country__c_label">Issuing country</label>
                        </section>
                        <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                         <div class="slds-form-element__control" style="width:30%;">
                             <select class="slds-select" id="app1_issuing_Country__c" name="app1_issuing_Country__c"/>
                         </div>
                        </section>
                    </div>
                </div>
            </div>
            <!-- END OF PRIMARY IDENTIFICATION -->
    
            <!-- START OF SECONDARY IDENTIFICATION -->
            <div class="slds-hide" id="app1_div_secondary_identification">
                <div class="slds-grid slds-wrap slds-m-vertical--xx-small">
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--left">
                          <label class="slds-form-element__label"></label>
                    </section>
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                         <h3 class="slds-text-heading--small slds-m-vertical--small">Secondary Identification</h3>
                    </section>
                </div>
                <div class="slds-grid slds-wrap slds-m-vertical--xx-small">
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                        <label class="slds-form-element__label" for="app1_id_1__c">Identification</label>
                    </section>
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                     <div class="slds-form-element__control" style="width:30%;">
                         <select class="slds-select isPicklistSpace" id="app1_id_1__c" name="app1_id_1__c"/>
                     </div>
                    </section>
                </div>
                <div class="slds-grid slds-wrap slds-m-vertical--xx-small">
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                          <label class="slds-form-element__label req" for="app1_id_no__c">ID no.</label>
                    </section>
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                        <div class="slds-form-element__control" style="width:60%;">
                           <input id="app1_id_no__c" name="app1_id_no__c" class="slds-input isRequired isAlphaNumericOnly isMaxLength16" type="text"/>
                        </div>
                    </section>
                </div>
                <div class="slds-grid slds-wrap slds-m-vertical--xx-small">
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                       <label class="slds-form-element__label">Issue date</label>
                    </section>
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                        <div class="slds-form-element__control">
                            <select class="slds-select" id="app1_sec_issue_dd" name="app1_sec_issue_dd" style="width:20%;"/>  
                            <select class="slds-select" id="app1_sec_issue_months" name="app1_sec_issue_months" style="width:20%;"/>  
                            <input id="app1_sec_issue_dy" name="app1_sec_issue_dy" class="slds-input isIssueDateSec" type="text" style="width:19%;" onkeypress="return isNumberKey(event)" maxlength="4"/>
                        </div>
                    </section>
                </div>
                <div class="slds-grid slds-wrap slds-m-vertical--xx-small">
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                       <label class="slds-form-element__label">Expiry date</label>
                    </section>
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                        <div class="slds-form-element__control" style="width:100%;">
                             <select class="slds-select app1_sec_expiry_dd" id="app1_sec_expiry_dd" name="app1_sec_expiry_dd" style="width:20%;"/>
                             <select class="slds-select app1_sec_expiry_months" id="app1_sec_expiry_months" name="app1_sec_expiry_months" style="width:20%;"/>
                             <input id="app1_sec_exp_dy" name="app1_sec_exp_dy" class="slds-input app1_sec_exp_dy isSecondaryExpiryDate" type="text" style="width:19%;" onkeypress="return isNumberKey(event)" maxlength="4"/>
                        </div>
                    </section>
                </div>
                <div class="slds-grid slds-wrap slds-m-vertical--xx-small">
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                          <label class="slds-form-element__label" for="app1_sec_id_issue_country__c">Issuing country</label>
                    </section>
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                     <div class="slds-form-element__control" style="width:30%;">
                         <select class="slds-select" id="app1_sec_id_issue_country__c" name="app1_sec_id_issue_country__c"/>
                     </div>
                    </section>
                </div>
                <div class="slds-grid slds-wrap slds-m-vertical--xx-small">
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                          <label class="slds-form-element__label" for="app1_sec_id_issuer__c">Issuer</label>
                    </section>
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
                        <div class="slds-form-element__control" style="width:60%">
                           <input id="app1_sec_id_issuer__c" name="app1_sec_id_issuer__c" class="slds-input issuerLength14" type="text"/>
                        </div>
                    </section>
                </div>
            </div>
            <!-- END OF SECONDARY IDENTIFICATION -->
            <div class="{!if($CurrentPage.parameters.country='AU', 'slds-show', 'slds-hide')}" id="div_drivers_licence" >
                <div class="slds-grid slds-wrap slds-m-vertical--xx-small">
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                       <label class="slds-form-element__label" for="app1_drivers_lic_no__c">Drivers Licence</label>
                    </section>
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--left">
                        <div class="slds-form-element__control" style="width:90%;">
                             <input id="app1_drivers_lic_no__c" name="app1_drivers_lic_no__c" class="slds-input isRequired isMaxLength10 isAUNumeric" type="text" onkeypress="return isAlphaNumericKey(event)"/>
                        </div>
                    </section>
                    <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--2-of-4 slds-text-align--left">
                        <div class="slds-form-element__control">
                            <label class="slds-checkbox" for="app1_drivers_lic_flg__c">
                                <input type="checkbox" id="app1_drivers_lic_flg__c" name="app1_drivers_lic_flg__c" onclick="disableDriversLicence(this, '1');" />
                                <span class="slds-checkbox--faux"></span>
                                <span class="slds-form-element__label">No driver's licence</span>
                            </label>
                        </div> 
                    </section>
                </div>
            </div>
            <div class="slds-grid slds-wrap slds-m-vertical--xx-small">
                <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4 slds-text-align--right">
                       <label class="slds-form-element__label req" for="app1_mobile__c">Mobile</label>
                </section>
                <section class="slds-col slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-4 slds-text-align--left">
