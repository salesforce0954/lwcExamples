                        aplcnt.Service_Delivery_Type__c = getValue('app'+i+'_service_delivery_type__c');   
                    }
                    
                    aplcnt.Service_Delivery_Id__c =   getValue('app'+i+'_service_delivery_id__c');
                    aplcnt.City_Pos__c =              getValue('app'+i+'_city_pos__c');
                    
                    if(aplcnt.Service_Delivery_Type__c != 'Residential'){
                    
                        var postalPropertyName = aplcnt.Service_Delivery_Type__c;

                        if(postalPropertyName == 'Community Mail Box'){
                            postalPropertyName = 'CMB';
                        }
                        
                        postalPropertyName += ' '+aplcnt.Service_Delivery_Id__c;
                        aplcnt.Postal_Property_Name__c = postalPropertyName;
                    }
                }
                
                aplcnt.Accept_Terms_and_Conditions__c = termsAndConditions;
                
                //Income
                aplcnt.Primary_Employment_Status__c =        getValue('app'+i+'_Primary_Employment_Status__c');
                aplcnt.Previous_Employment_Status__c =       getValue('app'+i+'_Previous_Employment_Status__c');
                aplcnt.Prev_Emp_Bus_Name__c =                getValue('app'+i+'_Prev_Emp_Bus_Name__c');
                
                //MLASALA: 05-APR-2016 - DEFECT 472 FIX CHANGED CHECKER TO TO EMPTY STRING WITHOUT SPACE BECAUSE FIELD IS CHANGED TO INPUT FROM SELECT
                // if(getValue('app'+i+'_Time_at_previous_employer_years__c') != ' ' && getValue('app'+i+'_Time_at_previous_employer_years__c') != '  '){ // Mar-22-2016 Afreen Updated for user story 131, changing picklist value to text
                if(getValue('app'+i+'_Time_at_previous_employer_years__c') != ''){
                    aplcnt.Time_at_previous_employer_years__c =  getValue('app'+i+'_Time_at_previous_employer_years__c');
                }
                
                if(getValue('app'+i+'_Time_at_previous_employer_months__c') != ''){ // May-05-2016 Afreen fix pack two , changing months picklist to text
                    aplcnt.Time_at_previous_employer_months__c = getValue('app'+i+'_Time_at_previous_employer_months__c');
                }
                    
                //Expense
                aplcnt.Res_Status__c = getValue('app'+i+'_Res_Status__c');
                
                //For primary applicant
                if(i == 1){
                    aplcnt.Is_Primary_Applicant__c = true;
                    
                    //Debt
                    aplcnt.Debt_Consolidation_Acknowledgement__c = getValue('app'+i+'_Debt_Consolidation_Acknowledgement__c');
                    
                    aplcnt.Total_Debts__c =   (Number(totalGEDebtAmount)+Number(totalOtherDebtAmount)).toFixed(2);
                    aplcnt.Total_Assets__c =  getTotalAmount('assetSection', 'app1_Asset_Value__c');
                    
                } else {
                    aplcnt.Is_Primary_Applicant__c = false;
                }

                //Get total amounts per applicant
                aplcnt.Total_Income__c =  getTotalAmount('app'+i+'_incomeSection', 'app'+i+'_totalIncomeAmount');                
                aplcnt.Total_Expense__c = getTotalAmount('expenseApplicant'+i, 'app'+i+'_totalExpenseAmount');

                //Length of income records
                var recordLength = $('.app'+i+'_incomeSection').length;
                var incomeAmounts = [];

                //Iterate over records and add to array
                    for(var inc=1; inc<=recordLength; inc++){
                        //incomeAmounts.push($('#app'+i+'_Income_Amount__c'+inc).val());  
                        console.log('Income Added');
                        console.log($('#app'+i+'_totalIncomeAmount'+inc).text());
                        incomeAmounts.push($('#app'+i+'_totalIncomeAmount'+inc).text());
                    }
                
                console.log('******INCOME**************');
                console.log(aplcnt.Total_Income__c);
                console.log(incomeAmounts);
                console.log(Math.max.apply(Math, incomeAmounts));
                
                //Get highest income record and subtract from total income per applicant
                aplcnt.Additional_Income__c = aplcnt.Total_Income__c - Math.max.apply(Math, incomeAmounts);              
                aplcnt.Application__c = app.Id;
                applicant.push(aplcnt);
            }
            console.log('applicant: '+applicant);
            debugLog('Applicant Data',applicant);

            // Create applicant/s
            createRecord(applicant);
        }
        
        /*  Description: Create date
         *  Author: Mike Lasala
         *  Date Created: 04-NOV-2015
         *  Input: Date
                   Month
                   Year
         */
        function createDate(date, month, year){
            
            var tempDate;
            
            if(date != '' && 
               month != '' && 
               year != ''){
                
                tempDate = new Date(month+'/'+date+'/'+year);
            }
            return tempDate;
        }
        
        /*  Description: Map aml record and fields to corresponding 
                         values from applicant which will be used for insert
         *  Author: Mike Lasala
         *  Date Created: 04-NOV-2015
         *  Input: None
         */
        function mapAML(){
            
            amls.length = 0;
            
            //Iterate over applicant/s
            for(var i=1; i<=applicant.length; i++){
                
                if(getValue('app'+i+'_customer_id_type__c') != ' ' && 
                   countryCom == 'NZ'){
                
                    //Instantiate new custom object/s
                    var aml = new sforce.SObject('AML__c');
                    
                    aml.Applicant__c =           applicant[i-1].Id;
                    aml.ID_No__c =               getValue('app'+i+'_id_number__c');
                    aml.Identification__c =      getValue('app'+i+'_customer_id_type__c');
                    aml.ID_1__c =                getValue('app'+i+'_customer_id_type__c');
                    aml.Issue_Country__c =       getValue('app'+i+'_issuing_Country__c');
                    aml.ID_Issue_Date__c =       createDate(getValue('app'+i+'_issue_dd'), 
                                                            getValue('app'+i+'_issue_months'), 
                                                            getValue('app'+i+'_issue_dy'));
                    aml.ID_Expiry_Date__c =      createDate(getValue('app'+i+'_expiry_dd'), 
                                                            getValue('app'+i+'_expiry_months'), 
                                                            getValue('app'+i+'_exp_dy'));
                    aml.Id_Sequence_No__c =      0;
                    aml.Type_of_Identity__c =    'Pre submit identification';
                    aml.Version__c =             getValue('app'+i+'_version__c');
                    aml.NZ_Residency_Number__c = getValue('app'+i+'_nz_residency_number__c');
                    
                    if(aml.Identification__c == 'NZ Birth Certificate' || 
                       aml.Identification__c == 'NZ Certificate of Citizenship'){
                        
                        var aml2 = new sforce.SObject('AML__c');
                        
                        aml2.Applicant__c =        applicant[i-1].Id;
                        aml2.ID_No__c =            getValue('app'+i+'_id_no__c');
                        aml2.Identification__c =   getValue('app'+i+'_id_1__c');
                        aml2.ID_2__c =             getValue('app'+i+'_id_1__c');
                        aml2.Issue_Country__c =    getValue('app'+i+'_sec_id_issue_country__c');
                        aml2.ID_Issue_Date__c =    createDate(getValue('app'+i+'_sec_issue_dd'), 
                                                              getValue('app'+i+'_sec_issue_months'), 
                                                              getValue('app'+i+'_sec_issue_dy'));
                        aml2.ID_Expiry_Date__c =   createDate(getValue('app'+i+'_sec_expiry_dd'), 
                                                              getValue('app'+i+'_sec_expiry_months'), 
                                                              getValue('app'+i+'_sec_exp_dy'));
                        aml2.Id_Sequence_No__c =   0;
                        aml2.Type_of_Identity__c = 'Pre submit identification';
                        aml2.ID_Issuer__c =        getValue('app'+i+'_sec_id_issuer__c');
                    
                        amls.push(aml2);
                    }
                    
                    amls.push(aml);
                }
            }
            
            console.log('amls: '+amls);
            debugLog('AMLs Data',amls);
        }
        /*  Description: Select correct street type value based on text
         *  Author: Davey Yu
         *  Date Created: NOV-19-2015
         *  Input Parameters: el -  Street type picklist element
         *  Input Parameters: text -  Street type value returned from data tools
        */  
        function returnSelectedIndex(el, text){
            for(var i=0; i<el.options.length; i++){
                if(el.options[i].text == text){
                    el.selectedIndex = i;
                    break;
                }
            }
        }   
