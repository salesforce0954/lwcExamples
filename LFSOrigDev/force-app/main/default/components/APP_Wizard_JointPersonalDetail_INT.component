            var pos_country         = $('#app'+appNum+'_country_pos__c');
            var pos_state           = $('#app'+appNum+'_state_pos__c');
            
            if(countryCom == 'NZ'){
                var res_city            = $('#app'+appNum+'_city_res__c');
                var pos_city            = $('#app'+appNum+'_city_pos__c');
            }
            
            if(elem.text == 'Yes'){
                //copy residential address details to postal address fields 
                pos_property_name.val(res_property_name.val());
                pos_unit_no.val(res_unit_no.val());
                pos_street_no.val(res_street_no.val());
                pos_street.val(res_street.val());
                pos_streetType.val(res_streetType.val());
                pos_suburb.val(res_suburb.val());
                pos_postcode.val(res_postcode.val());
                pos_country.val(res_country.val());
                pos_state.val(res_state.val());
                
