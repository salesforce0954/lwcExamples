        /*  Description: Map application record and fields to corresponding 
                         values in the form which will be used for insert
         *  Author: Mike Lasala
         *  Date Created: 29-OCT-2015
         *  Input: None
         */
        function createApplicant(){
            applicant.length = 0;
            var applicantCount = 1;
                
            //Identify if single or joint
            if(app.Application_Type__c == 'Joint'){
                applicantCount = 2;
            }
            
            //Iterate over applicant/s
            for(var i=1; i<=applicantCount; i++){
                    
                //Instantiate new custom object/s
                var aplcnt = new sforce.SObject('Applicant__c');
                    
                //Map common fields
                if(countryCom == 'NZ'){
                        if(getValue('lumSumConfirm') != '' && i == 1){
                        console.log('lumpsum11111111111111$$$$');
                            aplcnt.is_Lump_Sum_Debt__c = getValue('lumSumConfirm');
                        }
                        if(i ==2){
                            console.log('lumpsum22222222222$$$$');
                            aplcnt.is_Lump_Sum_Debt__c = false;
                        }
                            aplcnt.Income_Redundancy__c = getValue('incomeChangesValue'+i);    
                            aplcnt.Revolving_Credit__c = getValue('app1_Revolving_Credit__c');
              }
                //Eligibility                
                aplcnt.Meets_Eligibility_Criteria__c = getValue('app'+i+'_Meets_Eligibility_Criteria__c');
                aplcnt.Agrees_to_Privacy_Policy__c =   getValue('app'+i+'_Agrees_to_Privacy_Policy__c');
                <!-- Commented as part of NZ Insurance Stopsell --> 
               // aplcnt.Loan_Insurance_Consent__c =     getValue('app'+i+'_Loan_Insurance_Consent__c');
                
                <!-- BEGIN ARSHAD: 25/05/2018 -  BDR-105 -->
                if(countryCom == 'AU'){
                aplcnt.Consent_to_Lpi__c                = getValue('app'+i+'_Consent_to_Lpi__c');
                 <!-- Added below line for POI Automation Project by Suneel on 21/01/2021    --> 
                aplcnt.Agrees_to_Income_Verification__c = getValue('app'+i+'_Agrees_to_Income_Verification__c'); 
                <!-- END   ARSHAD: 25/05/2018 -  BDR-105 --> 
                }
               
                var birthDate =  getValue('app'+i+'_dob_dd');
                var birthMonth = getValue('app'+i+'_dob_months');
                var birthYear =  getValue('app'+i+'_dy');
                var noOfDeps =   getValue('app'+i+'_no_of_deps__c');

                //Personal details
                aplcnt.Title__c =       getValue('app'+i+'_title__c');
                aplcnt.First_Name__c =  getValue('app'+i+'_first_name__c');
                aplcnt.Middle_Name__c = getValue('app'+i+'_middle_name__c');
                aplcnt.Last_Name__c =   getValue('app'+i+'_last_name__c');
                aplcnt.Gender__c =      getValue('app'+i+'_gender__c');

                if(birthDate != '' && 
                   birthMonth != '' && 
                   birthYear != ''){
                    
                    aplcnt.Date_of_Birth__c =         formatDateToString(birthDate, birthMonth, birthYear, '');
                    aplcnt.Date_of_Birth_Doc_Gen__c = formatDateToString(birthDate, birthMonth, birthYear, 'DocGen');
                    aplcnt.Date_of_Birth_WS__c =      formatDateToString(birthDate, birthMonth, birthYear, 'WS');
                    aplcnt.Applicant_Age__c =         calculateAge(formatDateToString(birthDate, birthMonth, birthYear, 'DocGen'));
                }

                aplcnt.Mobile__c =                getValue('app'+i+'_mobile__c');
                aplcnt.Rel_Status__c =            getValue('app'+i+'_rel_status__c');
                aplcnt.No_of_Deps__c =            (noOfDeps != '') ? noOfDeps : 0;
                aplcnt.Home__c =                  getValue('app'+i+'_home__c');
                aplcnt.Work__c =                  getValue('app'+i+'_work__c');
                aplcnt.Email_Address__c =         getValue('app'+i+'_email_address__c').toLowerCase();
                aplcnt.Pref_Contact_Method__c =   getValue('app'+i+'_pref_contact_method__c');
                aplcnt.Home_Area_Code__c =        aplcnt.Home__c.substring(0,2);
                aplcnt.Home_WS__c =               aplcnt.Home__c.substring(2);
                aplcnt.Work_Area_Code__c =        aplcnt.Work__c.substring(0,2);
                aplcnt.Work_WS__c =               aplcnt.Work__c.substring(2);

                //Residential address
                aplcnt.Residential_Property_Name__c = getValue('app'+i+'_residential_property_name__c');
                aplcnt.Unit_No_Res__c =               getValue('app'+i+'_unit_no_res__c');
                aplcnt.Street_No_Res__c =             getValue('app'+i+'_street_no_res__c');
                aplcnt.Street_Res__c =                getValue('app'+i+'_street_res__c');
                aplcnt.Street_Type_Res__c =           getValue('app'+i+'_street_type_res__c');
                aplcnt.Suburb_Res__c =                getValue('app'+i+'_res_suburb_res__c');
                aplcnt.Postcode_Res__c =              getValue('app'+i+'_postcode_res__c');
                aplcnt.Country_Res__c =               getValue('app'+i+'_country_res__c');
                aplcnt.Years_At_Addr__c =             getValue('app'+i+'_years_at_addr__c');
                aplcnt.Months_At_Addr__c =            getValue('app'+i+'_months_at_addr__c');
                
                //We need to default the time at prev address to 0, otherwise if the time not provided, generate document extract will error out
                aplcnt.Years_At_Prev_Addr__c = 0;
                aplcnt.Months_At_Prev_Addr__c = 0;

                if((aplcnt.Years_At_Addr__c < 3 && 
                    countryCom == 'AU') || 

                   (aplcnt.Years_At_Addr__c < 2 && 
                    countryCom == 'NZ')){
                    
                    //Previous address
                    aplcnt.Previous_Property_Name__c = getValue('app'+i+'_previous_property_name__c');
                    aplcnt.Unit_No_Pre__c =            getValue('app'+i+'_unit_no_pre__c');
                    aplcnt.Street_No_Pre__c =          getValue('app'+i+'_street_no_pre__c');
                    aplcnt.Street_Pre__c =             getValue('app'+i+'_street_pre__c');
                    aplcnt.Street_Type_Pre__c =        getValue('app'+i+'_street_type_pre__c');
                    aplcnt.Suburb_Pre__c =             getValue('app'+i+'_suburb_pre__c');
                    aplcnt.Postcode_Pre__c =           getValue('app'+i+'_postcode_pre__c');
                    aplcnt.Country_Pre__c =            getValue('app'+i+'_country_pre__c');
                    aplcnt.Years_At_Prev_Addr__c =     getValue('app'+i+'_years_at_prev_addr__c');
                    aplcnt.Months_At_Prev_Addr__c =    getValue('app'+i+'_months_at_prev_addr__c');

                    if(countryCom == 'AU'){
                        aplcnt.State_Pre__c = getValue('app'+i+'_state_pre__c');
                    } else {
                        aplcnt.City_Pre__c = getValue('app'+i+'_city_pre__c');
                    }
                }

                //Postal address
                if($('#app'+i+'_postal_no').attr('class') == 'slds-button slds-button--brand'){
                    aplcnt.Postal_Addr_Flg__c = true;                    
                }
                
                aplcnt.Postal_Property_Name__c = getValue('app'+i+'_postal_property_name__c');
                aplcnt.Unit_No_Pos__c =          getValue('app'+i+'_unit_No_Pos__c');
                aplcnt.Street_No_Pos__c =        getValue('app'+i+'_street_no_pos__c');
                aplcnt.Street_Pos__c =           getValue('app'+i+'_street_pos__c');
                aplcnt.Street_Type_Pos__c =      getValue('app'+i+'_street_type_pos__c');
                aplcnt.Suburb_Pos__c =           getValue('app'+i+'_suburb_pos__c');
                aplcnt.Postcode_Pos__c =         getValue('app'+i+'_postcode_pos__c');
                aplcnt.Country_Pos__c =          getValue('app'+i+'_country_pos__c');
                
                var termsAndConditions = false;
                
                //Region specific fields
                if(countryCom == 'AU'){
                    
                    aplcnt.Agrees_to_Fees__c =  getValue('app'+i+'_Agrees_to_Fees__c');
                    aplcnt.EIDV__c =            getValue('app'+i+'_EIDV__c') ? 'Yes' : 'No';
                    aplcnt.Drivers_Lic_No__c =  getValue('app'+i+'_drivers_lic_no__c');
                    aplcnt.Drivers_Lic_Flg__c = getValue('app'+i+'_drivers_lic_flg__c');
                    aplcnt.PO_Box__c =          getValue('app'+i+'_po_box__c');
                    aplcnt.State_Pos__c =       getValue('app'+i+'_state_pos__c');
                    aplcnt.State_Res__c =       getValue('app'+i+'_state_res__c');
                    
                    if(aplcnt.Meets_Eligibility_Criteria__c && 
                       aplcnt.Agrees_to_Fees__c && 
                       aplcnt.Agrees_to_Privacy_Policy__c){
                        termsAndConditions = true;
                    }
                    
                } else {
                    
                    if(aplcnt.Meets_Eligibility_Criteria__c){
                        termsAndConditions = true;
                    }
                    
                    //Brand specific fields
                    if(app.Brand_String__c == 'Kiwibank'){
                        aplcnt.Kiwi_Bank_Access_Number__c = getValue('app'+i+'_Kiwi_Bank_Access_Number__c');
                    }
                    
                    if(getValue('app'+i+'_customer_id_type__c') != ' '){
                    
                        //Pre-submit identification
                        aplcnt.Customer_ID_Type__c =    getValue('app'+i+'_customer_id_type__c');
                        aplcnt.ID_Number__c =           getValue('app'+i+'_id_number__c');
                        aplcnt.Version__c =             getValue('app'+i+'_version__c');
                        aplcnt.NZ_Residency_Number__c = getValue('app'+i+'_nz_residency_number__c');
                        aplcnt.Issuing_Country__c =     getValue('app'+i+'_issuing_Country__c');
                        aplcnt.Issue_Date__c =          createDate(getValue('app'+i+'_issue_dd'), 
                                                                   getValue('app'+i+'_issue_months'), 
                                                                   getValue('app'+i+'_issue_dy'));
                        aplcnt.Expiry_Date__c =         createDate(getValue('app'+i+'_expiry_dd'), 
                                                                   getValue('app'+i+'_expiry_months'), 
                                                                   getValue('app'+i+'_exp_dy'));
                        
                        if(aplcnt.Customer_ID_Type__c == 'NZ Birth Certificate' || 
                           aplcnt.Customer_ID_Type__c == 'NZ Certificate of Citizenship'){
                            
                            aplcnt.ID_No__c =                getValue('app'+i+'_id_no__c');
                            aplcnt.ID_1__c =                 getValue('app'+i+'_id_1__c');
                            aplcnt.Sec_ID_Issue_Country__c = getValue('app'+i+'_sec_id_issue_country__c');
