                yes.attr('class','slds-button slds-button--brand');
                no.attr('class','slds-button slds-button--neutral');
                sec_pos_address.hide();
            }else{
                no.attr('class','slds-button slds-button--brand');
                yes.attr('class','slds-button slds-button--neutral');
