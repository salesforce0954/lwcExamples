        /******************************Data Tool*************************************/
        /*  Description: Calling Datatool service to get address for auto pop
         *  Author: Tony Xu
         *  Date Created: 2015-10-12
         *  Input Parameters: component - id of the address input field that needs auto pop
         */    
        function Datatool(component){
            //Initialize variables depends on country
            var url = "https://dt0200.datatoolscloud.net.au/KleberWebService/DtKleberService.svc/ProcessQueryStringRequest"
            var methodSearch = countryCom == "AU" ? "DataTools.Capture.Address.Predictive.AuPaf.SearchAddress" : "DataTools.Capture.Address.Predictive.NzPaf.SearchAddress";
            var methodRetrieve = countryCom == "AU" ? "DataTools.Capture.Address.Predictive.AuPaf.RetrieveAddress" : "DataTools.Capture.Address.Predictive.NzPaf.RetrieveAddress";
            var requestKey = "RK-50C90-4C675-C53A0-7D997-A72AA-A2D0E-BB686-78C49";
            var departmentCode = "CloudSherp";
            $(component).autocomplete({
                source: function(request,response){
                    $.ajax({
                       url: url,
                       dataType: "jsonp",
                       type: "GET", 
                       contentType: "application/json; charset=utf-8",
                       data: {OutputFormat:"json", ResultLimit:10, AddressLine:request.term, Method:methodSearch, RequestKey:requestKey, DepartmentCode:departmentCode}, 
                       success: function( data ) {  
                           console.log(data);
                           response( $.map( data.DtResponse.Result, function( item ) {
                                var Output = countryCom == "AU" ? (item.AddressLine + ", " + item.Locality + ", " + item.State + ", " + item.Postcode) : (item.AddressLine + ", " + item.Suburb + ", " + item.TownCityMailtown + ", " + item.Postcode);
                                return {
                                    label: Output ,
                                    value: Output ,
                                    Output: Output,
                                    RecordId: item.RecordId, 
                                    AddressLine: item.AddressLine
                                }
                            }));
                        } 
                    });      
                },
                select: function(event, ui){
                    $.ajax({
                        url: url,
                        dataType: "jsonp",
                        type: "GET",
                        contentType: "application/json; charset=utf-8",
                        data: {Method:methodRetrieve, RecordId:ui.item.RecordId, RequestId:"", RequestKey:requestKey, DepartmentCode:departmentCode, OutputFormat:"json" },
                        success: function(data){
                            console.log(data.DtResponse.Result);
                            $.map(data.DtResponse.Result, function(item){
                                console.log(item.UnitNumber);
                                console.log('ITEM IS: ',item);
                                AutoPopulateAddress(component,item,countryCom);
                            });
                        }    
                    });
                }
            });
        }
            
            
