                            aplcnt.Sec_ID_Issue_Date__c =    createDate(getValue('app'+i+'_sec_issue_dd'), 
                                                                        getValue('app'+i+'_sec_issue_months'), 
                                                                        getValue('app'+i+'_sec_issue_dy'));
                            aplcnt.Sec_ID_Expiry_Date__c =   createDate(getValue('app'+i+'_sec_expiry_dd'), 
                                                                        getValue('app'+i+'_sec_expiry_months'), 
                                                                        getValue('app'+i+'_sec_exp_dy'));
                        }
                    }
                    
                    
                    aplcnt.City_Res__c =              getValue('app'+i+'_city_res__c');
                            
                    if($('#app'+i+'_postal_yes').attr('class').indexOf('brand') > -1){
                        aplcnt.Service_Delivery_Type__c = 'Residential';
