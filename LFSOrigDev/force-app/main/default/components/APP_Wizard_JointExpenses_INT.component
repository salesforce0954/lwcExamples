                                //Calculate Personal expense amount
                                totalExpenseAmount += calculateInterval(Number($('#app'+i+'_Personal_Other_Expense__c').val()), $('#app'+i+'_Personal_Other_Expense_Interval__c').val());
                            }

                            if(Number($('#app'+i+'_Any_Additional_expenses__c').val()) >= 0){
                                
                                //Calculate Any Additional expense amount
                                totalExpenseAmount += calculateInterval(Number($('#app'+i+'_Any_Additional_expenses__c').val()), $('#app'+i+'_Any_Additional_expenses_Interval__c').val());
                            }
                        
                        }
                        
                        if(residentialStatus == 'Renting' || 
                           residentialStatus == 'Boarding with Parents' || 
                           residentialStatus == 'Boarding Other'){
                            
                            if(Number($('#app'+i+'_Rent_Board_Pay_Amt__c').val()) >= 0){
                            
                                //Calculate rent expense amount
                                totalExpenseAmount += calculateInterval(Number($('#app'+i+'_Rent_Board_Pay_Amt__c').val()), $('#app'+i+'_Rent_Board_Pay_Int__c').val());
                            }
                        }
                    }
                    
                    totalExpenseAmount = Number(totalExpenseAmount).toFixed(2);  

                    $('#app'+i+'_totalExpenseAmount1').text('$'+addCommas(totalExpenseAmount));
                    
                }
            }
            
