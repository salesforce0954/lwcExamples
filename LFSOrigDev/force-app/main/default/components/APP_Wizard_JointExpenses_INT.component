            /*  Description: Toggle sections based on residential status
             *  Author: Mike Lasala
             *  Date Created: 07-OCT-2015
             *  Input: Residential Status, 
                       Applicant
             */
            function showHideExpense(residentialStatus, applicant){
                
                //Identify expense element (applicant 1 or 2)
                var expense = $('#Applicant'+applicant+'ExpenseSection');
                
                //Clear fields
                $('#app'+applicant+'_I_Pay_All_Exp__c').val(' ');
                $('#app'+applicant+'_Living_Exp__c').val('');
                $('#app'+applicant+'_Living_Exp_Int__c').val(' ');
                $('#app'+applicant+'_Rent_Board_Pay_Amt__c').val('');
                $('#app'+applicant+'_Rent_Board_Pay_Int__c').val('');
                $('#app'+applicant+'_Agent_Landlord_Name__c').val('');
                    
                if(residentialStatus == 'Renting' || 
                   residentialStatus == 'Boarding with Parents' || 
                   residentialStatus == 'Boarding Other'){
                    
                    //Clear rent fields
                    $('#app'+applicant+'_Rent_Board_Pay_Amt__c').val('');
                    $('#app'+applicant+'_Rent_Board_Pay_Int__c').val('');
                    $('#app'+applicant+'_Agent_Landlord_Name__c').val('');
                    
                    if(countryComp == 'AU'){
                    //Show rent section
                    expense.find('#rentSection').show();
                    }
                    
                    if(residentialStatus == 'Renting'){
                    
                        //Show agent/landlord section
                        // if(country == 'AU')
                            expense.find('#agentSection').show();
                    } else {
                    
                        //Hide agent/landlord section
                        // if(country == 'AU')
                            expense.find('#agentSection').hide();
                    }
                    
                } else {
                
                    //Hide rent section
                    expense.find('#rentSection').hide();
                    
                    //Hide agent/landlord section
                    expense.find('#agentSection').hide();
                }
                
                calculateTotalExpenseAmount();
            }
                       
            
            /*  Description: Calculate total expense amount
             *  Author: Mike Lasala
             *  Date Created: 19-OCT-2015
             *  Input: None
             */
            function calculateTotalExpenseAmount(){
                
                var totalExpenseAmount;
                var applicants = 1;
                
                //Identify if application is joint by checking if expense 2 section is displayed
                
              /* Commenting the expense section for Applicant 2
               *  if($('.expenseApplicant2').css('display') == 'block'){
               *     applicants = 2;
               * }
               */
                
                //Iterate over each applicant's expense record
                for(var i=1; i<=applicants; i++){
                    
                    totalExpenseAmount = 0;
                    
                    //Identify residential status
                    var residentialStatus = $('#app'+i+'_Res_Status__c').val();
                        
                    if(residentialStatus != ' '){
                    
                        if(countryComp == 'AU'){
                           
                            if(Number($('#app'+i+'_Living_Exp__c').val()) >= 0){
                                
                                //Calculate living expense amount
                                totalExpenseAmount += calculateInterval(Number($('#app'+i+'_Living_Exp__c').val()), $('#app'+i+'_Living_Exp_Int__c').val());
