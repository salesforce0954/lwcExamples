                            }

                            if(Number($('#app'+i+'_Food_Groc_Entert__c').val()) >= 0){
                                
                                //Calculate food groceries entertainment expense amount
                                totalExpenseAmount += calculateInterval(Number($('#app'+i+'_Food_Groc_Entert__c').val()), $('#app'+i+'_Food_Groc_Interval__c').val());
