                            }

                            if(Number($('#app'+i+'_Secondary_Mortgage_Rent__c').val()) >= 0){
                                
                                //Calculate Secondary Mortgage expense amount
                                totalExpenseAmount += calculateInterval(Number($('#app'+i+'_Secondary_Mortgage_Rent__c').val()), $('#app'+i+'_Secondary_Mortgage_Rent_Interval__c').val());
                                console.log(' Secondary Mortgate rent,board Amount $$ '+totalExpenseAmount);
                            }

                            if(Number($('#app'+i+'_Property_rates__c').val()) >= 0){
                                
                                //Calculate Mortgage expense amount
                                totalExpenseAmount += calculateInterval(Number($('#app'+i+'_Property_rates__c').val()), $('#app'+i+'_Property_rates_Interval__c').val());
                                console.log('Property Rates Amount $$ '+totalExpenseAmount);
                            }

                            if(Number($('#app'+i+'_Other_Property_Expenses__c').val()) >= 0){
                                
                                //Calculate Mortgage expense amount
                                totalExpenseAmount += calculateInterval(Number($('#app'+i+'_Other_Property_Expenses__c').val()), $('#app'+i+'_Other_Property_Expenses_Interval__c').val());
                                console.log('Other Property Rates Amount $$ '+totalExpenseAmount);
                            }

                            if(Number($('#app'+i+'_Food_Groc_Entert__c').val()) >= 0){
                                
                                //Calculate food groceries entertainment expense amount
                                totalExpenseAmount += calculateInterval(Number($('#app'+i+'_Food_Groc_Entert__c').val()), $('#app'+i+'_Food_Groc_Interval__c').val());
                                console.log('Food Groc Expense Amount $$ '+totalExpenseAmount);
                            }

                            if(Number($('#app'+i+'_Insurance_Expense__c').val()) >= 0){
                                
                                //Calculate insurance expense amount
                                totalExpenseAmount += calculateInterval(Number($('#app'+i+'_Insurance_Expense__c').val()), $('#app'+i+'_Insurance_Expense_Interval__c').val());
                            }

                            if(Number($('#app'+i+'_Utilities_Expense__c').val()) >= 0){
                                
                                //Calculate utilities expense amount
                                totalExpenseAmount += calculateInterval(Number($('#app'+i+'_Utilities_Expense__c').val()), $('#app'+i+'_Utilities_Expense_Interval__c').val());
                            }

                            if(Number($('#app'+i+'_Transport_Expense__c').val()) >= 0){
                                
                                //Calculate Transport expense amount
                                totalExpenseAmount += calculateInterval(Number($('#app'+i+'_Transport_Expense__c').val()), $('#app'+i+'_Transport_Expense_Interval__c').val());
                            }

                            if(Number($('#app'+i+'_Education_and_child_Expense__c').val()) >= 0){
                                
